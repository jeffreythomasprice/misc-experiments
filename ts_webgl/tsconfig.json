{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}