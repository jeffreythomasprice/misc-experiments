{
	"editor.codeActionsOnSave": {
		"source.fixAll": true,
		"fixAll.eslint": true,
		"source.organizeImports": true,
	},
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"dist": true,
	}
}