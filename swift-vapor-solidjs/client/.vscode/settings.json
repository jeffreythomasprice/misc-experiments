{
	"editor.insertSpaces": true,
	"editor.tabSize": 4,
	"editor.codeActionsOnSave": {
		"source.fixAll": "always",
		"fixAll.eslint": "always",
		"source.organizeImports": "always"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"dist": true,
	}
}